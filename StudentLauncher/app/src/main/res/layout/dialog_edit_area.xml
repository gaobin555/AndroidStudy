<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="200dp"
    android:layout_margin="@dimen/margin_large"
    android:background="@drawable/shape_black_rect_bg"
    android:orientation="vertical"
    android:padding="@dimen/padding">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginBottom="@dimen/margin_large"
        android:layout_marginLeft="@dimen/margin_xlarge"
        android:layout_marginRight="@dimen/margin_xlarge"
        android:layout_weight="1"
        android:orientation="horizontal">

        <com.android.xthink.ink.launcherink.common.view.WheelView
            android:id="@+id/dialog_edit_date_province"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:divider="@color/primary"
            android:dividerHeight="1dp" />

        <com.android.xthink.ink.launcherink.common.view.WheelView
            android:id="@+id/dialog_edit_date_city"
            android:layout_width="0dp"
            android:layout_height="100dp"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:divider="@color/primary"
            android:dividerHeight="1dp" />


    </LinearLayout>

    <include layout="@layout/common_dialog_bottom" />

</LinearLayout>