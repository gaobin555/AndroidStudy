<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RobotoTextView">
        <attr name="fontType" format="enum">
            <enum name="light" value="0" />
            <enum name="medium" value="1" />
            <enum name="regular" value="2" />
            <enum name="bold" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CircularClockSeekBar">
        <attr name="hand_hour" format="reference" />
        <attr name="hand_minute" format="reference" />
    </declare-styleable>

    <attr name="fromDegrees" format="float|reference" />
    <attr name="toDegrees" format="float|reference" />
    <attr name="childSize" format="dimension|reference" />

    <declare-styleable name="ArcLayout">
        <attr name="fromDegrees" />
        <attr name="toDegrees" />
        <attr name="childSize" />
    </declare-styleable>

    <declare-styleable name="ArcMenu">
        <attr name="fromDegrees" />
        <attr name="toDegrees" />
        <attr name="childSize" />
    </declare-styleable>

    <declare-styleable name="SwitchView">
        <attr name="isOpen" format="boolean" />
    </declare-styleable>
    <declare-styleable name="SettingView">
        <attr name="title" format="string" />
        <attr name="des" format="string" />
        <attr name="open" format="boolean" />
        <attr name="isUpdate" format="boolean" />
    </declare-styleable>
</resources>
